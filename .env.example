# Copy this file to .env and add your real API keys

# Claude API Key from console.anthropic.com  
CLAUDE_API_KEY=your_claude_api_key_here

# Twitter OAuth 2.0 Client Credentials from developer.twitter.com
TWITTER_CLIENT_ID=your_client_id_here
TWITTER_CLIENT_SECRET=your_client_secret_here
TWITTER_CALLBACK_URL=http://localhost:3000/callback

# Twitter OAuth 2.0 Tokens (get these from running oauth flow)
TWITTER_ACCESS_TOKEN=your_access_token_here
TWITTER_REFRESH_TOKEN=your_refresh_token_here
